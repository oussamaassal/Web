  <script>
    function cardsCenter() {
      jQuery('.card-slider').owlCarousel({
        loop:true,
        margin:0,
        nav:true,
        slideSpeed: 3000,
        paginationSpeed: 3000,
        dots: true,
        navText: ['<i class="fas fa-arrow-left"></i>', '<i class="fas fa-arrow-right"></i>'],
        responsive:{
          0:{ items:1 },
          576:{ items:1 },
          800:{ items:1 },
          991:{ items:1 },
          1200:{ items:1 },
          1600:{ items:1 }
        }
      });
    }

    jQuery(window).on('load', function() {
      setTimeout(function() {
        cardsCenter();
      }, 1000);
    });
  </script>
